name: cubical-experiments
include: .
depend:
    standard-library
    cubical
flags:
    --cubical
    --no-import-sorts
    --postfix-projections
    --hidden-argument-puns
    --allow-unsolved-metas
    --rewriting
    --guardedness
    --erasure
    -W noInteractionMetaBoundaries
    -W noUnsupportedIndexedMatch
